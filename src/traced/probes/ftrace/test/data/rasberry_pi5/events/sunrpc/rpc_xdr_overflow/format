name: rpc_xdr_overflow
ID: 1228
format:
	field:unsigned short common_type;	offset:0;	size:2;	signed:0;
	field:unsigned char common_flags;	offset:2;	size:1;	signed:0;
	field:unsigned char common_preempt_count;	offset:3;	size:1;	signed:0;
	field:int common_pid;	offset:4;	size:4;	signed:1;

	field:unsigned int task_id;	offset:8;	size:4;	signed:0;
	field:unsigned int client_id;	offset:12;	size:4;	signed:0;
	field:int version;	offset:16;	size:4;	signed:1;
	field:size_t requested;	offset:24;	size:8;	signed:0;
	field:const void * end;	offset:32;	size:8;	signed:0;
	field:const void * p;	offset:40;	size:8;	signed:0;
	field:const void * head_base;	offset:48;	size:8;	signed:0;
	field:size_t head_len;	offset:56;	size:8;	signed:0;
	field:const void * tail_base;	offset:64;	size:8;	signed:0;
	field:size_t tail_len;	offset:72;	size:8;	signed:0;
	field:unsigned int page_len;	offset:80;	size:4;	signed:0;
	field:unsigned int len;	offset:84;	size:4;	signed:0;
	field:__data_loc char[] progname;	offset:88;	size:4;	signed:0;
	field:__data_loc char[] procedure;	offset:92;	size:4;	signed:0;

print fmt: "task:%08x@%08x %sv%d %s requested=%zu p=%p end=%p xdr=[%p,%zu]/%u/[%p,%zu]/%u
", REC->task_id, REC->client_id, __get_str(progname), REC->version, __get_str(procedure), REC->requested, REC->p, REC->end, REC->head_base, REC->head_len, REC->page_len, REC->tail_base, REC->tail_len, REC->len
