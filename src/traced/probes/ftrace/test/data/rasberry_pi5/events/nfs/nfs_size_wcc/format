name: nfs_size_wcc
ID: 568
format:
	field:unsigned short common_type;	offset:0;	size:2;	signed:0;
	field:unsigned char common_flags;	offset:2;	size:1;	signed:0;
	field:unsigned char common_preempt_count;	offset:3;	size:1;	signed:0;
	field:int common_pid;	offset:4;	size:4;	signed:1;

	field:dev_t dev;	offset:8;	size:4;	signed:0;
	field:u32 fhandle;	offset:12;	size:4;	signed:0;
	field:u64 fileid;	offset:16;	size:8;	signed:0;
	field:u64 version;	offset:24;	size:8;	signed:0;
	field:loff_t cur_size;	offset:32;	size:8;	signed:1;
	field:loff_t new_size;	offset:40;	size:8;	signed:1;

print fmt: "fileid=%02x:%02x:%llu fhandle=0x%08x version=%llu cursize=%lld newsize=%lld", ((unsigned int) ((REC->dev) >> 20)), ((unsigned int) ((REC->dev) & ((1U << 20) - 1))), (unsigned long long)REC->fileid, REC->fhandle, REC->version, REC->cur_size, REC->new_size
