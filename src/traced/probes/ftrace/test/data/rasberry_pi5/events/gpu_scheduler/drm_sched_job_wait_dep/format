name: drm_sched_job_wait_dep
ID: 1327
format:
	field:unsigned short common_type;	offset:0;	size:2;	signed:0;
	field:unsigned char common_flags;	offset:2;	size:1;	signed:0;
	field:unsigned char common_preempt_count;	offset:3;	size:1;	signed:0;
	field:int common_pid;	offset:4;	size:4;	signed:1;

	field:__data_loc char[] name;	offset:8;	size:4;	signed:0;
	field:uint64_t id;	offset:16;	size:8;	signed:0;
	field:struct dma_fence * fence;	offset:24;	size:8;	signed:0;
	field:uint64_t ctx;	offset:32;	size:8;	signed:0;
	field:unsigned seqno;	offset:40;	size:4;	signed:0;

