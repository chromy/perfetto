name: rdev_color_change
ID: 1419
format:
	field:unsigned short common_type;	offset:0;	size:2;	signed:0;
	field:unsigned char common_flags;	offset:2;	size:1;	signed:0;
	field:unsigned char common_preempt_count;	offset:3;	size:1;	signed:0;
	field:int common_pid;	offset:4;	size:4;	signed:1;

	field:char wiphy_name[32];	offset:8;	size:32;	signed:0;
	field:char name[16];	offset:40;	size:16;	signed:0;
	field:int ifindex;	offset:56;	size:4;	signed:1;
	field:u8 count;	offset:60;	size:1;	signed:0;
	field:u16 bcn_ofs;	offset:62;	size:2;	signed:0;
	field:u16 pres_ofs;	offset:64;	size:2;	signed:0;

print fmt: "%s, netdev:%s(%d), count: %u", REC->wiphy_name, REC->name, REC->ifindex, REC->count
