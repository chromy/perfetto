name: rpc_xdr_recvfrom
ID: 1275
format:
	field:unsigned short common_type;	offset:0;	size:2;	signed:0;
	field:unsigned char common_flags;	offset:2;	size:1;	signed:0;
	field:unsigned char common_preempt_count;	offset:3;	size:1;	signed:0;
	field:int common_pid;	offset:4;	size:4;	signed:1;

	field:unsigned int task_id;	offset:8;	size:4;	signed:0;
	field:unsigned int client_id;	offset:12;	size:4;	signed:0;
	field:const void * head_base;	offset:16;	size:8;	signed:0;
	field:size_t head_len;	offset:24;	size:8;	signed:0;
	field:const void * tail_base;	offset:32;	size:8;	signed:0;
	field:size_t tail_len;	offset:40;	size:8;	signed:0;
	field:unsigned int page_base;	offset:48;	size:4;	signed:0;
	field:unsigned int page_len;	offset:52;	size:4;	signed:0;
	field:unsigned int msg_len;	offset:56;	size:4;	signed:0;

print fmt: "task:%08x@%08x head=[%p,%zu] page=%u(%u) tail=[%p,%zu] len=%u", REC->task_id, REC->client_id, REC->head_base, REC->head_len, REC->page_len, REC->page_base, REC->tail_base, REC->tail_len, REC->msg_len
